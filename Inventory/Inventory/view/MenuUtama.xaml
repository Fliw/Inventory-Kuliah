<Window x:Class="Inventory.view.MenuUtama"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Inventory.view"
        mc:Ignorable="d"
        Title="MenuUtama" Height="553.5" Width="800">
    <Grid Background="White">
        <Label Content="Aplikasi Inventory" HorizontalAlignment="Left" Margin="48,10,0,0" VerticalAlignment="Top" FontWeight="Bold" FontSize="20" Foreground="#FF29BFEE"/>
        <Canvas HorizontalAlignment="Left" Height="146" Margin="181,47,0,0" VerticalAlignment="Top" Width="611" Background="#FF29BFEE">
            <Canvas Height="62" Canvas.Left="26" Canvas.Top="26" Width="122" Background="White">
                <Image Height="84" Canvas.Left="54" Canvas.Top="-17" Width="88" Source="../assets/Total Transaksi (5).png"/>
            </Canvas>
            <Label Content="50" Canvas.Left="26" Canvas.Top="26" FontWeight="Bold" Foreground="#FF29BFEE"/>
            <Label Content="Total Barang Keluar" Canvas.Left="26" Canvas.Top="62" FontWeight="Bold" Foreground="#FF29BFEE"/>
            <Canvas Height="62" Canvas.Left="153" Canvas.Top="26" Width="122" Background="White">
                <Label Content="80" FontWeight="Bold" Background="{x:Null}" Foreground="#FF29BFEE"/>
            </Canvas>
            <Label Content="Total Barang Masuk" Canvas.Left="153" Canvas.Top="62" FontWeight="Bold" Foreground="#FF29BFEE"/>
            <Image Height="86" Canvas.Left="168" Width="153" Source="../assets/Total Transaksi (6).png" Canvas.Top="10"/>
            <Canvas Height="62" Canvas.Left="280" Canvas.Top="26" Width="122" Background="White"/>
            <Label Content="10jt" Canvas.Left="280" Canvas.Top="26" FontWeight="Bold" Foreground="#FF29BFEE" RenderTransformOrigin="0.328,-0.058"/>
            <Label Content="Total Transaksi" Canvas.Left="280" Canvas.Top="62" Foreground="#FF29BFEE" FontWeight="Bold"/>
            <Image Height="70" Canvas.Left="304" Width="133" Source="../assets/Total Transaksi (11).png" Canvas.Top="10"/>
            <Canvas Height="62" Canvas.Left="407" Canvas.Top="26" Width="121" Background="White">
                <Label Content="5" FontWeight="Bold" Foreground="#FF29BFEE"/>
                <Label Content="Total Laporan" Canvas.Top="36" FontWeight="Bold" Foreground="#FF29BFEE"/>
            </Canvas>
            <Image Height="72" Canvas.Left="407" Width="170" Source="../assets/Total Transaksi (12).png" Canvas.Top="10"/>
        </Canvas>
        <Image HorizontalAlignment="Left" Height="28" Margin="10,68,0,0" VerticalAlignment="Top" Width="35" Source="../assets/icon-rumah.png">
            <Image.OpacityMask>
                <ImageBrush ImageSource="icon-rumah.png"/>
            </Image.OpacityMask>
        </Image>
        <Label Content="Beranda" HorizontalAlignment="Left" Margin="50,59,0,0" VerticalAlignment="Top" Foreground="#FF29BFEE" FontSize="20" FontWeight="Bold"/>
        <Image HorizontalAlignment="Left" Height="33" Margin="10,112,0,0" VerticalAlignment="Top" Width="35" Source="../assets/icon-data-check.png"/>
        <Label Content="Data Barang" HorizontalAlignment="Left" Margin="50,112,0,0" VerticalAlignment="Top" FontSize="20" FontWeight="Bold" Foreground="#FF29BFEE"/>
        <Image HorizontalAlignment="Left" Height="30" Margin="10,163,0,0" VerticalAlignment="Top" Width="35" Source="../assets/icon-transaksi2.png"/>
        <Label Content="Transaksi" HorizontalAlignment="Left" Margin="50,156,0,0" VerticalAlignment="Top" FontSize="20" FontWeight="Bold" Foreground="#FF29BFEE"/>
        <Image HorizontalAlignment="Left" Height="34" Margin="12,209,0,0" VerticalAlignment="Top" Width="33" Source="../assets/icon-laporan2.png"/>
        <Label Content="Laporan" HorizontalAlignment="Left" Margin="50,206,0,0" VerticalAlignment="Top" FontSize="20" FontWeight="Bold" Width="96" Foreground="#FF29BFEE"/>
        <Image HorizontalAlignment="Left" Height="36" Margin="12,263,0,0" VerticalAlignment="Top" Width="33" Source="../assets/icon-setting-akun2.png"/>
        <Label Content="Setting Akun" HorizontalAlignment="Left" Margin="50,263,0,0" VerticalAlignment="Top" FontSize="20" FontWeight="Bold" Foreground="#FF29BFEE"/>
        <Image HorizontalAlignment="Left" Height="277" Margin="182,241,0,0" VerticalAlignment="Top" Width="404" Source="../assets/Total Transaksi (2).png" RenderTransformOrigin="0.5,0.5">
            <Image.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.377"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Image.RenderTransform>
        </Image>
        <Image HorizontalAlignment="Left" Height="267" Margin="452,246,0,0" VerticalAlignment="Top" Width="340" Source="../assets/Total Transaksi.png"/>

    </Grid>
</Window>
