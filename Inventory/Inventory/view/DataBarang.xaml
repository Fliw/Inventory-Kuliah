        <Image HorizontalAlignment="Left" Height="26" VerticalAlignment="Top" Width="27" Source="../assets/icon-transaksi2.png" Margin="15,201,0,0"/>
        <Image HorizontalAlignment="Left" Height="26" VerticalAlignment="Top" Width="30" Source="../assets/icon-laporan2.png" Margin="16,280,0,0" RenderTransformOrigin="0.87,1.981"/>
        <Image HorizontalAlignment="Left" Height="26" VerticalAlignment="Top" Width="27" Source="../assets/icon-setting-akun2.png" Margin="12,438,0,0" RenderTransformOrigin="0.741,2.056"/>
